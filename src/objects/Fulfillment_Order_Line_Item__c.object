    <validationRules>
        <fullName>Edit_New_FF_ITEM_Closed</fullName>
        <active>true</active>
        <description>Prevents users from editing of creating a new FF item when closed.</description>
        <errorConditionFormula>AND
(OR
(AND
(ISPICKVAL(Fulfillment_Order__r.Status__c, &quot;Closed&quot;), 
ISNEW()), 
ISPICKVAL(Fulfillment_Order__r.Status__c, &quot;Closed&quot;), 
AND
(Fulfillment_Order__r.Locked_from_Editing__c = true)), 
NOT
($Profile.Name=&quot;Integration User&quot;))</errorConditionFormula>
        <errorMessage>Fulfillment Order is Closed, you can not add or edit items. Please Contact your System Administrator.</errorMessage>
    </validationRules>
