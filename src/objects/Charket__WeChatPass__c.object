    <validationRules>
        <fullName>Charket__Date_Type_Fix_Begin_Term_Limit</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL( Charket__DateType__c  ,&apos;Fixed Term&apos;), ISBLANK(Charket__FixedDaysValidAfter__c))</errorConditionFormula>
        <errorDisplayField>Charket__FixedDaysValidAfter__c</errorDisplayField>
        <errorMessage>You must enter a value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Charket__Date_Type_Fixed_Term_Limit</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL( Charket__DateType__c  ,&apos;Fixed Term&apos;), ISBLANK(Charket__FixedDaysValidFor__c))</errorConditionFormula>
        <errorDisplayField>Charket__FixedDaysValidFor__c</errorDisplayField>
        <errorMessage>You must enter a value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Charket__Date_Type_Fixed_Time_Range_Limit</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL( Charket__DateType__c  ,&apos;Fixed Time Range&apos;), ISBLANK(Charket__StartTime__c))</errorConditionFormula>
        <errorDisplayField>Charket__StartTime__c</errorDisplayField>
        <errorMessage>You must enter a value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Charket__Fixed_Term_Time_Limit</fullName>
        <active>true</active>
        <errorConditionFormula>Charket__StartTime__c  &gt; Charket__EndTime__c</errorConditionFormula>
        <errorDisplayField>Charket__StartTime__c</errorDisplayField>
        <errorMessage>The pass end time should not be before the start time.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Charket__Quantity_Limit</fullName>
        <active>true</active>
        <errorConditionFormula>Charket__LimitPerFollower__c  &gt;  Charket__Quantity__c</errorConditionFormula>
        <errorMessage>The Limit Per Follower can&apos;t larger than Quantity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Charket__Title_Length_Limit</fullName>
        <active>true</active>
        <errorConditionFormula>LEN( Name )  &gt; 27</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>The Title value can contain 27 characters at most.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Charket__Update_Base_Info_Limit</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
              OR(ISPICKVAL(Charket__Status__c, &apos;Approved&apos;), ISPICKVAL(Charket__Status__c, &apos;Delivered&apos;)),
              OR( ISCHANGED(Charket__WeChatAccount__c), ISCHANGED(Name),ISCHANGED(Charket__BrandName__c), ISCHANGED(Charket__CardId__c),
               ISCHANGED(Charket__Detail__c), ISCHANGED(Charket__FixedDaysValidAfter__c),ISCHANGED(Charket__FixedDaysValidFor__c),
               ISCHANGED(Charket__DateType__c))
            )</errorConditionFormula>
        <errorMessage>These fields (WeChat Account, Title, Brand Name, Card Id, Detail, Fixed Begin Term, Fixed Term, Date Type) can&apos;t be edit when this pass has been approved.</errorMessage>
    </validationRules>
