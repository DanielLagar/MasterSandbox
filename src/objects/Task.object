    <validationRules>
        <fullName>Stop_agent_changing_follow_up_category</fullName>
        <active>true</active>
        <errorConditionFormula>(ISCHANGED(Follow_Up_Category__c) || ISCHANGED(Follow_Up_Reason__c)) 
&amp;&amp; 
(OR(NOT(ISBLANK(CCM_Followup_Id__c)), NOT(ISBLANK(CCM_Task_ID__c))))</errorConditionFormula>
        <errorMessage>The Follow Up Category or Follow Up Reason cannot be changed once a CCM Task/Followup ID has been assigned to the task</errorMessage>
    </validationRules>
