    <validationRules>
        <fullName>Article_Number_Format</fullName>
        <active>true</active>
        <description>prevents incorrect article number format being entered</description>
        <errorConditionFormula>/*example: REGEX(SSN__c, &quot;[0-9]{3}-[0-9]{2}-[0-9]{4}&quot;)*/
OR(
NOT(
REGEX(Featured_Article_1__c, &quot;[0-9]{9}|^$&quot;)
),
NOT(
REGEX(Featured_Article_2__c, &quot;[0-9]{9}|^$&quot;)
),
NOT(
REGEX(Featured_Article_3__c, &quot;[0-9]{9}|^$&quot;)
),
NOT(
REGEX(Featured_Article_4__c, &quot;[0-9]{9}|^$&quot;)
),
NOT(
REGEX(Featured_Article_5__c, &quot;[0-9]{9}|^$&quot;)
))</errorConditionFormula>
        <errorMessage>The article number format you have entered is incorrect. Please check the article number and try again. An article number should look like &quot;000046513&quot;.</errorMessage>
    </validationRules>
